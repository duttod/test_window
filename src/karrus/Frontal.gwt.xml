<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='frontal'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<stylesheet src='syncro.css' />
	<inherits name="gwtupload.GWTUpload" />
	<inherits name='ca.nanometrics.gflot.GFlot' />
<!-- 	<inherits name='ca.nanometrics.gflot.GFlotJQueryNoConflict' /> -->
	<inherits name='com.google.gwt.widgetideas.WidgetIdeas' />
	<inherits name="com.google.gwt.gen2.commonwidget.CommonWidget" />
	<inherits name="com.google.gwt.gen2.table.override.Override" />
	<inherits name="com.google.gwt.gen2.table.ScrollTable" />
	<inherits name='net.sf.gilead.Gilead4Gwt' />
	<inherits name="de.novanic.eventservice.GWTEventService"/>
	<inherits name="com.reveregroup.gwt.imagepreloader.ImagePreloader" />
	<inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> 
	<inherits name="com.allen_sauer.gwt.log.gwt-log-RemoteLogger" />
	<set-property name="log_WindowLogger" value="DISABLED" />
	<set-property name="log_ConsoleLogger" value="ENABLED" />
	<set-property name="log_DivLogger" value="DISABLED" />
	<set-property name="log_FirebugLogger" value="DISABLED" />
	<set-property name="log_GWTLogger" value="ENABLED" />
	<set-property name="log_SystemLogger" value="ENABLED" />
	<set-property name="compiler.emulatedStack" value="true" />
	<set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true"/>
	<set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true"/>
	<add-linker name="xsiframe"/>
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
	
	<!-- Pour pouvoir utiliser l'internationalisation -->
	<inherits name="com.google.gwt.i18n.I18N" />

	<!-- Pour dire que sa langue locale est le francais Attention Ã  mettre les 
		deux lignes -->
	<extend-property name="locale" values="fr" />
	<set-property name="locale" value="fr" />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='karrus.client.FrontalWebApp'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
